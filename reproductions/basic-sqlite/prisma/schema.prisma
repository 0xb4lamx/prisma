datasource db {
  provider = "postgresql"
  url      = "postgres://prisma:prisma@localhost:5432/db"
}

generator client {
  provider        = "prisma-client-js"
  previewFeatures = ["interactiveTransactions"]
  output          = "../node_modules/.prisma/client"
}

model User {
  id    String @id @default(uuid())
  email String @unique
}
